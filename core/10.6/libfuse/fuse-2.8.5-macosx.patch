+    int ret;
+    struct fuse_fs *fs;
+    struct threadid *d;
+
+    d = calloc(1, sizeof(*d));
+    if (d == NULL) {
+        fprintf(stderr, "threadid: memory allocation failed\n");
+        return NULL;
+    }
+
+    if (fuse_opt_parse(args, d, threadid_opts, threadid_opt_proc) == -1) {
